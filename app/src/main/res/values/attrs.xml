<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SelectableViewGroup">
        <attr name="divider" format="reference" />
        <attr name="selectedIndex" format="integer" />
        <attr name="itemsTitle" format="reference" />
        <attr name="itemsIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="ItemView">
        <attr name="itemTint" format="color" />
        <attr name="itemText" format="string" />
